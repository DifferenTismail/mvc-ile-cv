
@{
    ViewBag.Title = "DeneyimEkle";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h4 class="mb-4">Yeni Deneyim Ekleme Sayfası</h4>
<form method="post" class="form-group">
    <input type="text" name="Baslik" class="form-control" placeholder="Başlığı Yazınız"/><br />
    <input type="text" name="AltBaslik" class="form-control" placeholder="Alt Başlık"/><br />
    <input type="text" name="Tarih" class="form-control" placeholder="Tarih"/><br />
    <textarea placeholder="Açıklamanızı Giriniz" name="Aciklama" class="form-control"></textarea><br />
    <button class="btn btn-info">Kaydet</button>
</form>
